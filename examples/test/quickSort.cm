def main() {
    let arr = [10, 80, 30, 90, 40, 50, 70]
    quick_sort(arr, 0, 6)
}

def print_arr(arr: Int[], n: Int) {
    for let i in 0..<n {
        print(arr[i])
    }
}

def quick_sort(arr: Int[], low: Int, high: Int) {
    let p = partition(arr, low, high)
    print(p)
}

def partition(arr: Int[], low: Int, high: Int) -> Int {
    let pivot = arr[high]
    let i = low - 1
    for let j in low..<high {
        if arr[j] < pivot or arr[j] == pivot {
            i = i + 1
            let tmp = arr[j]
            arr[j] = arr[i]
            arr[i] = tmp
        }
    }
    let tmp = arr[i+1]
    arr[i+1] = arr[high]
    arr[high] = tmp
    return i + 1
}
